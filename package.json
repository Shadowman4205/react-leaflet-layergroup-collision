{
  "name": "react-leaflet-layergroup-collision",
  "version": "1.0.0",
  "description": "Layergroup Collision as React compoment for Leaflet",
  "main": "dist/react-leaflet-layergroup-collision.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean:lib": "rm -Rf ./lib",
    "compile:lib": "babel src --out-dir lib",
    "build:lib": "npm run clean:lib && npm run compile:lib",
    "build:dist": "webpack src/index.js dist/react-leaflet-layergroup-collision.js & webpack -p src/index.js dist/react-leaflet-layergroup-collision.min.js",
    "build": "npm run build:lib && npm run build:dist",
    "example": "webpack-dev-server --config ./example/webpack.config.js",
    "lint": "eslint ./src",
    "release:major": "npm version major && git push --follow-tags",
    "release:minor": "npm version minor && git push --follow-tags",
    "release:patch": "npm version patch && git push --follow-tags",
    "publish": "npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:Shadowman4205/react-leaflet-layergroup-collision.git"
  },
  "keywords": [
    "react",
    "leaflet",
    "bezier",
    "layergroup",
    "collision"
  ],
  "author": "EPOZ",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Shadowman4205/react-leaflet-layergroup-collision/issues"
  },
  "homepage": "https://github.com/Shadowman4205/react-leaflet-layergroup-collision#readme",
  "peerDependencies": {
    "leaflet": "^1.0.2",
    "react-leaflet": "^1.0.1",
    "react": "^15.4.1"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.1.18",
    "eslint": "^3.8.1",
    "eslint-plugin-react": "^6.4.1",
    "leaflet": "^1.0.2",
    "lodash.isequal": "^4.4.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-leaflet": "^1.0.1",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.12.1"
  },
  "dependencies": {
    "rbush": "^2.0.1"
  }
